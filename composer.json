{
  "name": "lochmueller/fl_realurl_image",
  "type": "typo3-cms-extension",
  "version": "4.0.0",
  "description": "Add the RealURL functionality to image files. 'typo3temp/2d972d5c89b5.jpg' goes 'nice-name.jpg'! Many different fallbacks like file reference, file, content element or page settings to get the right file name.",
  "autoload": {
    "psr-4": {
      "FRUIT\\FlRealurlImage\\": "Classes/"
    }
  },
  "keywords": [
    "TYPO3 CMS",
    "Realurl"
  ],
  "authors": [
    {
      "name": "Tim LochmÃ¼ller",
      "email": "webmaster@fruit-lab.de",
      "role": "Developer",
      "homepage": "https://github.com/lochmueller"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "typo3/cms-core": "~6.2.14|>=7.4.0,<9.0"
  },
  "replace": {
    "fl_realurl_image": "self.version",
    "typo3-ter/fl-realurl-image": "self.version"
  },
  "homepage": "https://github.com/lochmueller/fl_realurl_image",
  "license": "GPL-2.0+",
  "support": {
    "issues": "https://github.com/lochmueller/fl_realurl_image/issues"
  }
